{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "global-require": 0,
        "no-param-reassign": 0,
        "no-restricted-syntax": 0,
        "prefer-spread": 0,
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "prefer-rest-params": 0
    }
}